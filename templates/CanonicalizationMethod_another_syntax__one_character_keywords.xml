<xd:CanonicalizationMethod xd1:Algorithm="http://www.w3.org/2010/xml-c14n2">
	<x-:IgnoreComments>( $ignore_comments )</x-:IgnoreComments>
	<x-:TrimTextNodes>( $trim_text_nodes )</x-:TrimTextNodes>
	<x-:PrefixRewrite>( $prefix_rewrite )</x-:PrefixRewrite>
	<x-:QNameAware>
		<!--You have a CHOICE of the next 4 items at this level-->
		<x-:Element x-:Name="string" x-:NS="( ?$qname_aware_element )"/>
		<x-:XPathElement x-:Name="string" x-:NS="( $qname_aware_xpath_element )"/>
		<x-:QualifiedAttr x-:Name="string" x-:NS="( ?$qname_aware_qualified_attr )"/>
		<x-:UnqualifiedAttr x-:Name="string" x-:ParentName="string" x-:ParentNS="( ?$qname_aware_unqualified_attr )"/>
	</x-:QNameAware>
</xd:CanonicalizationMethod>