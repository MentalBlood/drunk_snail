<xe:EncryptedKey wsu:Id="string">
	<xe:EncryptionMethod xe:Algorithm="( $encryption_method )"/>
	<xe:KeyInfo wsu:Id="string">
		<xd:X509Data>
			<!--You have a CHOICE of the next 3 items at this level-->
			<xd:X509Certificate>( ?$x509_certificate )</xd:X509Certificate>
			<xd:X509SKI>( ?$x509_ski )</xd:X509SKI>
			( ?~IssuerSerial )
		</xd:X509Data>
	</xe:KeyInfo>
	<xe:CipherData>
		<xe:CipherValue>( $cipher_value )</xe:CipherValue>
	</xe:CipherData>
	<xe:ReferenceList>
		<!--1 or more repetitions:-->
		( ~DataReference )
	</xe:ReferenceList>
</xe:EncryptedKey>